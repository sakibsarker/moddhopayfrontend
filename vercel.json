{
    "version": 2,
    "builds": [
      {
        "build": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "build": "client/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "app": "/(.*)",
        "build": "/client/index.html"
      }
    ]
  }
  