{
  "version": 2,
  "builds": [
    {
      "src": "build/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "build/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "build/client/$1"
    }
  ]
}
