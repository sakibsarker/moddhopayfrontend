{
    "version": 2,
    "builds": [
      {
        "src": "build/server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/client/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/build/client/index.html"
      }
    ]
  }
  